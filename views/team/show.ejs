<h1 class="page_title">
  My profile
</h1>
<div class="row profile">
  <div class="sign_in_container challenge_panel profile_panel l3 m4 s12">
    <span class="flag flag-tn teams_item_flag"></span>
    <img src="/images/team/<%= team.id %>.png" class="item_avatar">
    <h3 class="profile_name"><%= team.name %></h3>
    <!--<div class="divider"/>-->
    <div class="profile_informations">
      <div class="profile_informations_item">
        <i class="fa fa-map-marker profile_informations_item_icon"></i>
        <span class="profile_informations_item_value">Tunisia</span>
      </div>
      <div class="profile_informations_item">
        <i class="fa fa-building profile_informations_item_icon"></i>
        <span class="profile_informations_item_value">ENIT</span>
      </div>
      <div class="profile_informations_item">
        <i class="fa fa-envelope profile_informations_item_icon"></i>
        <span class="profile_informations_item_value"><a href="mailto:contact@g2foss.org">contact@g2foss.org</a></span>
      </div>
      <% if(team.members){ %>
      <div class="profile_informations_item">
        <i class="fa fa-users profile_informations_item_icon"></i>
                    <span class="profile_informations_item_value">
                      <% team.members.split(' ').forEach(function(member){ %>
                      <span class="profile_informations_item_value_item truncate">Boubaya Mohamed</span>
                      <% }) %>
                    </span>
      </div>
      <% } %>
    </div>

  </div>

